(ns joker.test-joker.hash
  (:require
   [joker.test :refer [deftest is testing]]))

(deftest stable-hashes
  (testing "stable string hashes"
    (is (= (hash "hey") 4290027229))
    (is (= (hash "there") 3102463325)))
  (testing "stable symbol hashes"
    (is (= (hash 'hey) 2649495702))
    (is (= (hash 'there) 1461931798))
    (is (= (hash 'joker.core/cond) 2971988202))
    (is (= (hash 'joker.repl/doc) 1115223904))
    (is (= (hash 'user/foo) 229553053)))
  (testing "stable keyword hashes"
    (is (= (hash :hey) 4290027229))
    (is (= (hash :there) 3102463325))
    (is (= (hash ::you) 670624077))
    (is (= (hash :user/foo) 1870084580)))
  (testing "stable stdio hashes"
    (is (= (hash *in*) 533021371))
    (is (= (hash *out*) 4013866757))
    (is (= (hash *err*) 3215140612))))
