(ns deps
  (:require [joker.os :as os]
            [joker.string :as str]))

(def lib-dir
  (-> (os/args)
       (second)
       (str/split #"/")
       (butlast)
       (concat ["deps" "lib"])
       ((fn [x] (apply str (interpose "/" x))))))

(ns-sources
  {"test-local.*" {:url lib-dir}
   "test-http.*" {:url "https://s3.us-east-2.amazonaws.com/tech.public.data/"}})
