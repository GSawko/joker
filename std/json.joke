(ns
  ^{:go-imports []
    :doc "Implements encoding and decoding of JSON as defined in RFC 4627."}
  json)

(defn read-string
  "Parses the JSON-encoded data and return the result as a Joker value."
  {:added "1.0"
  :go "readString(s)"}
  [^String s])

(defn write-string
  "Returns the JSON encoding of v."
  {:added "1.0"
  :go "writeString(v)"}
  [^Object v])


